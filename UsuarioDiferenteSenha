import java.util.Scanner;

public class Main {
  
  
  public static void main(String[] args) {
    
    Scanner teclado = new Scanner(System.in);
    char user, senha;
    System.out.println("Coloque seu usu치rio: ");
    user = teclado.nextLine().charAt(0);
    System.out.println("Coloque sua senha: ");
    senha = teclado.nextLine().charAt(0);
    while (senha == user) {
      System.out.println("Senha inv치lida. Evite senhas iguais ao usu치rio: ");
      senha = teclado.nextLine().charAt(0);
    }
    if (senha != senha) {
      System.out.println("Usu치rio registrado com sucesso!");
    }
    
  }

}
